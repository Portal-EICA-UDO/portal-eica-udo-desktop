{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "paths": {
      "@shared/*": [
        "./src/shared/*"
      ],
      "@layouts/*": [
        "./src/shared/ui/layouts/*"
      ],
      "@app/*": [
        "./src/app/*"
      ]
    }
  }
}